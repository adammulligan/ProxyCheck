{
  "name": "ProxyCheck",
  "version": "1.1",
  "description": "Provides an easy, instantly accessible method of determining if you are behind a proxy.",
  "homepage_url": "http://cyanoryx.com",
  "icons": {
    "128": "assets/icon.png"
  },
  "browser_action": {
    "default_icon": "assets/icon_off.png",
    "popup": "popup.html"
  },
  "background_page": "background.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "<all_urls>"
  ]
}
